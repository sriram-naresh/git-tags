pipeline{
  stages{
    stage("checkout"){
      steps{
        Scripts{
	  checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'git-tga2', url: 'https://github.com/sriram-naresh/git-tags.git']]])
	  }
        }
     }
     stage("checkout"){
      steps{
        scripts{
         sh """
           mvn clean
           mvn install
         """
         }
       }
     }
  }
} 
